# vim:ft=plumed
phi: READ FILE=COLVAR VALUES=phi IGNORE_TIME
psi: READ FILE=COLVAR VALUES=psi IGNORE_TIME
nn: READ FILE=COLVAR VALUES=nn.bias IGNORE_TIME
w: REWEIGHT_BIAS TEMP=300. ARG=nn.bias
h: HISTOGRAM ARG=phi,psi GRID_MIN=-pi,-pi GRID_MAX=pi,pi GRID_BIN=300,300 BANDWIDTH=0.15,0.15 LOGWEIGHTS=w
fes: CONVERT_TO_FES GRID=h TEMP=300.
DUMPGRID GRID=fes FILE=reweight FMT=%24.16e
